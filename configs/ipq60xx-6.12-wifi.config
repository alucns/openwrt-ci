# ========== 优化的内核配置 ==========

# 生产环境关闭调试功能
CONFIG_KERNEL_DEBUG_INFO=n
CONFIG_KERNEL_DEBUG_INFO_REDUCED=n
CONFIG_KERNEL_DEBUG_KERNEL=n
CONFIG_KERNEL_DEBUG_RODATA=n
CONFIG_KERNEL_SLUB_DEBUG=n
CONFIG_KERNEL_DEBUG_VM=n

# 精简安全模块
CONFIG_KERNEL_SECURITY=y
CONFIG_KERNEL_SECURITY_SELINUX=n
CONFIG_KERNEL_SECURITY_APPARMOR=n
CONFIG_KERNEL_LSM_MMAP_MIN_ADDR=32768

# 文件系统精简（与模块配置保持一致）
CONFIG_KERNEL_FS_XFS=n
CONFIG_KERNEL_FS_BTRFS=n

# 内存管理优化
CONFIG_KERNEL_COMPACTION=y
CONFIG_KERNEL_PAGE_POISONING=n
CONFIG_KERNEL_PAGE_TABLE_ISOLATION=n
CONFIG_KERNEL_SLAB_FREELIST_RANDOM=n
CONFIG_KERNEL_SLAB_FREELIST_HARDENED=n

# TCP网络优化
